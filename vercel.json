  "name": "inkpath",
  "framework": "nextjs",
  "buildCommand": "npm install && npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "packagePath": "frontend/package.json",
  "env": {
    "NEXT_PUBLIC_DEMO_MODE": "true"
  }
}
